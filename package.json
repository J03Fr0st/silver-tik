{
  "name": "tiktok-follower-scraper",
  "version": "1.0.0",
  "description": "A Node.js application to scrape TikTok follower lists",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm src/index.ts",
    "dev": "nodemon --watch src -e ts --exec \"node --loader ts-node/esm src/index.ts\"",
    "build": "tsc"
  },
  "keywords": [
    "tiktok",
    "scraper",
    "followers",
    "puppeteer"
  ],
  "author": "You",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.7",
    "puppeteer": "^21.11.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "@types/node": "^20.17.19",
    "@types/puppeteer": "^5.4.7",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
